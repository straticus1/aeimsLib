# AEIMS Master Environment Configuration
# Unified configuration for aeims, aeims-control, and aeimsLib integration
# Generated using universal-agent-mcp-kit methodology

# ======================
# Infrastructure Settings
# ======================
ENVIRONMENT=production
AWS_REGION=us-east-1
AWS_PROFILE=aeims-production

# ======================
# Database Configuration
# ======================
# PostgreSQL for AEIMS Core
AEIMS_CORE_DB_NAME=aeims_core
AEIMS_CORE_DB_USER=aeims_user
AEIMS_CORE_DB_PASS=secure_password_123
AEIMS_CORE_DB_PORT=5432

# MySQL for AEIMS App
AEIMS_APP_DB_NAME=aeims_app
AEIMS_APP_DB_USER=aeims_user
AEIMS_APP_DB_PASS=secure_password_123
AEIMS_APP_DB_PORT=3306
AEIMS_APP_DB_ROOT_PASS=secure_root_pass_123

# Redis for all services
REDIS_PASSWORD=secure_redis_pass
REDIS_PORT=6379

# ======================
# Service URLs (Internal)
# ======================
AEIMS_CORE_URL=http://aeims-core:8000
AEIMS_APP_URL=http://aeims-app
AEIMS_LIB_URL=http://aeims-lib:8080

# ======================
# External Service URLs
# ======================
AEIMS_CORE_EXTERNAL_URL=https://core.aeims.app
AEIMS_APP_EXTERNAL_URL=https://aeims.app
AEIMS_LIB_EXTERNAL_URL=https://lib.aeims.app

# ======================
# Authentication & Security
# ======================
JWT_SECRET=shared_jwt_secret_key_change_in_production
JWT_EXPIRY=24h
JWT_REFRESH_EXPIRY=7d

# Encryption
ENCRYPTION_KEY=aes256_encryption_key_change_in_production
ENCRYPTION_ALGORITHM=aes-256-gcm

# ======================
# WebSocket Configuration
# ======================
WEBSOCKET_PORT=8080
WEBSOCKET_HOST=0.0.0.0
WS_HEARTBEAT_INTERVAL=30000
WS_CONNECTION_TIMEOUT=60000

# ======================
# VoIP Configuration
# ======================
ASTERISK_HOST=asterisk.aeims.app
ASTERISK_PORT=5060
ASTERISK_USERNAME=aeims_asterisk
ASTERISK_PASSWORD=secure_asterisk_pass

# AWS Connect
AWS_CONNECT_INSTANCE_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
AWS_CONNECT_CONTACT_FLOW_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# ======================
# Payment Processing
# ======================
STRIPE_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxxxxxxx
STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxxxxxxx
PAYPAL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
PAYPAL_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxx

# ======================
# Monitoring & Logging
# ======================
LOG_LEVEL=info
LOG_RETENTION_DAYS=30
METRICS_ENABLED=true
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001
GRAFANA_ADMIN_PASSWORD=admin123

# ======================
# Docker & Deployment
# ======================
NODE_ENV=production
BUILD_TARGET=production
BUILD_DATE=2024-01-01T00:00:00Z

# Docker ports
AEIMS_CORE_PORT=8000
AEIMS_APP_PORT=80
AEIMS_APP_SSL_PORT=443
AEIMS_LIB_PORT=8081
AEIMS_FRONTEND_PORT=3000
NGINX_HTTP_PORT=8082
NGINX_HTTPS_PORT=8443

# ======================
# Device Control (aeimsLib)
# ======================
DEVICE_CONTROL_ENABLED=true
DEVICE_SESSIONS_TIMEOUT=300000
DEVICE_COMMAND_RATE_LIMIT=10
DEVICE_MAX_CONCURRENT_SESSIONS=100

# Supported protocols
LOVENSE_API_ENABLED=true
WEVIBE_API_ENABLED=true
KIIROO_API_ENABLED=true
BUTTPLUG_PROTOCOL_ENABLED=true

# ======================
# Third-party APIs
# ======================
# SMS/Email
TWILIO_ACCOUNT_SID=xxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxx
SENDGRID_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Analytics
GOOGLE_ANALYTICS_ID=UA-xxxxxxxxx-x
MIXPANEL_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxx

# ======================
# Compliance & Security
# ======================
AGE_VERIFICATION_ENABLED=true
CONTENT_MODERATION_ENABLED=true
GDPR_COMPLIANCE_ENABLED=true
CCPA_COMPLIANCE_ENABLED=true

# Rate limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_SKIP_FAILED_REQUESTS=false

# ======================
# Development Settings
# ======================
DEBUG=false
ENABLE_SWAGGER=false
CORS_ORIGIN=https://aeims.app,https://api.aeims.app
TRUST_PROXY=true

# ======================
# Version Information
# ======================
AEIMS_VERSION=1.0.0
AEIMS_BUILD_NUMBER=1
AEIMS_COMMIT_SHA=main